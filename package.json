  "keywords": [
    "english",
    "korean",
    "macos",
    "typo",
    "convert",
    "fix",
    "translate",
    "hangul"
  ],
  "author": "Geonu Kim <laziu.cc@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*",
